{
  "name": "expressjs-rest-template",
  "version": "0.1.0",
  "description": "expressjs-rest-template",
  "main": "server.js",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test mocha --require @babel/register --exit ./src/api/**/*.test.js",
    "start": "nodemon --exec babel-node src/server.js",
    "debug": "nodemon --inspect-brk=0.0.0.0:9229 --nolazy --exec babel-node src/server.js",
    "build": "npm run clean && babel src --copy-files --out-dir dist --ignore src/client",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "serve": "node dist/server.js",
    "clean": "rimraf dist",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "db": "babel-node src/db_seed"
  },
  "author": "Uyen Do",
  "license": "ISC",
  "homepage": "https://bitbucket.org/minhuyen/expressjs-rest-template#readme",
  "dependencies": {
    "api-query-params": "^5.1.1",
    "apn": "^1.6.2",
    "aws-sdk": "^2.387.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "celebrate": "^10.0.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "ejs": "^3.1.8",
    "express": "^4.16.4",
    "express-validator": "^6.14.0",
    "helmet": "^3.21.2",
    "http-status": "^1.3.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.4.3",
    "mongoose-delete": "^0.5.0",
    "mongoose-paginate-v2": "^1.3.18",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.16",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "redis": "^4.0.1",
    "socket.io": "^4.5.0",
    "socketio-jwt": "^4.5.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2",
    "util": "^0.11.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "concurrently": "^5.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^28.1.0",
    "mocha": "^8.1.3",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  }
}
